{
    "version": "2.0.0",
	"type": "shell",
	"command": "nim",
	"nim.lintOnSave": true,
	"nim.buildCommand": "c",
	"nim.buildOnSave": false,
	"taskName": "Run module.nim",
	"nim.licenseString": "# Copyright 2017.\n\n",
    "options": {
		"cwd": "${workspaceRoot}"
	},
	"group": {
		"kind": "build",
		"isDefault": true
	},
	"nim.project": [
        "example.nim",
        "problem.nim"
    ],
	"args": [
        "c",
        "-r",
        "module.nim"
    ],
    "tasks": [{
        "group": "build",
        "type": "cppbuild",
        "detail": "Task generated by Debugger.",
        "label": "C/C++: g++.exe build active file",
        "command": "C:\\msys64\\mingw64\\bin\\g++.exe",
        "presentation": {
            "reveal": "silent"
        },
        "options": {
            "cwd": "${fileDirname}"
        },
        "problemMatcher": [
            "$gcc",
            "$msCompile"
        ],
        "args": [
            "-g",
            "-o",
            "a.exe",
            "code.cpp"
        ],
    }],
}
{
	"semi": true,
	"tabWidth": 2,
	"useTabs": true,
	"printWidth": 100,
	"endOfLine": "crlf",
	"proseWrap": "never",
	"singleQuote": false,
	"trailingComma": "all",
	"jsxSingleQuote": false,
	"bracketSameLine": true,
	"bracketSpacing": false,
	"quoteProps": "as-needed",
	"jsxBracketSameLine": true,
	"vueIndentScriptAndStyle": true,
	"htmlWhitespaceSensitivity": "css",
}
